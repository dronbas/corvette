{
  "extends": ["tslint-config-prettier", "tslint-plugin-prettier"],
  "linterOptions": {
    "exclude": ["**/*.d.ts", "**/node_modules/**/*.ts"]
  },
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "printWidth": 120,
        "trailingComma": "all"
      }
    ],
    "newline-before-return": true,
    "cyclomatic-complexity": [true, 10],
    "interface-name": true
  }
}
